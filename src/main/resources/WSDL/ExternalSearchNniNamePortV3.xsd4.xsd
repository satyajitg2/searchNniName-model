<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="uri:bn.types.asic.gov.au" elementFormDefault="qualified" xmlns:bn.types="uri:bn.types.asic.gov.au" xmlns:abn.types="uri:abn.types.asic.gov.au" xmlns:types="uri:types.asic.gov.au" xmlns:fss.types="uri:fss.types.asic.gov.au" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="uri:types.asic.gov.au" schemaLocation="ExternalSearchNniNamePortV3.xsd3.xsd"/>
  <xs:import namespace="uri:abn.types.asic.gov.au" schemaLocation="ExternalSearchNniNamePortV3.xsd5.xsd"/>
  <xs:import namespace="uri:fss.types.asic.gov.au" schemaLocation="ExternalSearchNniNamePortV3.xsd9.xsd"/>
  <xs:annotation>
    <xs:documentation>Contains various ASIC definitions of BN standard types</xs:documentation>
  </xs:annotation>
  <xs:complexType name="organisationLodgeType">
    <xs:sequence>
      <xs:element name="name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="200"/>
            <xs:pattern value="[A-Z0-9\.\\!\(\):;\?',\-&amp;&quot;=$%#\|/_\{\}\*@ ]*"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="acn" type="types:nniNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Company only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orgNumber" type="xs:string" minOccurs="0"/>
      <xs:element name="address" type="types:addressLodgeType" minOccurs="0" maxOccurs="99">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="emailAddress" type="types:emailType" minOccurs="0"/>
      <xs:element name="startDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="organisationType">
    <xs:sequence>
      <xs:element name="name">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="200"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="acn" type="types:nniNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Company only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orgNumber" type="xs:string" minOccurs="0"/>
      <xs:element name="address" type="types:addressExtType" minOccurs="0" maxOccurs="99">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="emailAddress" type="types:emailType" minOccurs="0"/>
      <xs:element name="startDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="urlType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="250"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="termType">
    <xs:restriction base="xs:string">
      <xs:length value="1"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="simpleBnExtractType">
    <xs:sequence>
      <xs:element name="business" type="bn.types:businessEntityType"/>
      <xs:element name="businessName" type="bn.types:outboundBusinessNameType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="businessNameExtractType">
    <xs:sequence>
      <xs:element name="business" minOccurs="0" maxOccurs="999">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="bn.types:businessEntityType">
              <xs:sequence>
                <xs:element name="startDate" type="xs:date" minOccurs="0"/>
                <xs:element name="endDate" type="xs:date" minOccurs="0"/>
                <xs:element name="agentAppointed" type="types:trueType" minOccurs="0"/>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="businessName" type="bn.types:outboundBusinessNameType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="documentType">
    <xs:sequence>
      <xs:element name="documentNumber" type="types:documentNoType"/>
      <xs:element name="dateReceived" type="xs:date"/>
      <xs:element name="formCode" type="types:codeType"/>
      <xs:element name="numberOfPages" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:maxInclusive value="9999"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="description" type="types:descriptionType"/>
      <xs:element name="additionalDescription" minOccurs="0" maxOccurs="99">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="subformCode" type="types:codeType" minOccurs="0"/>
            <xs:element name="subformDescription" type="types:descriptionType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="billingType">
    <xs:sequence>
      <xs:element name="charge">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="code" type="types:codeType"/>
            <xs:element name="description" type="types:descriptionType"/>
            <xs:element name="historical" type="xs:boolean" minOccurs="0"/>
            <xs:element name="large" type="xs:boolean" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="fees" type="fss.types:invoiceType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chargeType">
    <xs:sequence>
      <xs:element name="code" type="types:codeType"/>
      <xs:element name="description" type="types:descriptionType"/>
      <xs:element name="historical" type="xs:boolean" minOccurs="0"/>
      <xs:element name="large" type="xs:boolean" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="previousStateTerritoryType">
    <xs:sequence>
      <xs:element name="number">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
            <xs:pattern value="[A-Z0-9\.\\!\(\):;\?',\-&amp;&quot;=$%#\|/_\{\}\*@ ]*"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="stateTerritoryCode" type="types:stateTerritoryCodeType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="contactPersonType">
    <xs:sequence>
      <xs:element name="name" type="types:personNameLodgeType" minOccurs="0"/>
      <xs:element name="phoneNumber" type="types:telephoneNumberType" nillable="true"/>
      <xs:element name="emailAddress" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="200"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="contactPersonWithAddressType">
    <xs:complexContent>
      <xs:extension base="bn.types:contactPersonType">
        <xs:sequence>
          <xs:element name="address" type="types:addressType" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="outboundContactPersonType">
    <xs:complexContent>
      <xs:extension base="bn.types:contactPersonType">
        <xs:sequence>
          <xs:element name="startDate" type="xs:date" minOccurs="0"/>
          <xs:element name="endDate" type="xs:date" minOccurs="0"/>
          <xs:element name="documentNumber" type="types:documentNoType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>~Internal~</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="outboundContactPersonWithAddressType">
    <xs:complexContent>
      <xs:extension base="bn.types:outboundContactPersonType">
        <xs:sequence>
          <xs:element name="address" type="types:addressType" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="personWithRolesType">
    <xs:complexContent>
      <xs:extension base="types:personNameBirthType">
        <xs:sequence>
          <xs:element name="role" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="business" type="bn.types:businessEntityType"/>
                <xs:element name="businessName" type="bn.types:outboundBusinessNameType" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="type" type="bn.types:roleTypeType" minOccurs="0"/>
                <xs:element name="address" type="types:addressExtType" minOccurs="0"/>
                <xs:element name="startDate" type="xs:date" minOccurs="0"/>
                <xs:element name="endDate" type="xs:date" minOccurs="0"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="personsWithRolesType">
    <xs:sequence>
      <xs:element name="person" type="bn.types:personWithRolesType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bnCredentialsType">
    <xs:sequence>
      <xs:element name="number" type="types:nniNumberType"/>
      <xs:element name="key" type="types:keyType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="outboundBusinessNameType">
    <xs:sequence>
      <xs:element name="nniNumber" type="xs:string"/>
      <xs:element name="bnReferenceNumber" type="types:bnReferenceNumberType" minOccurs="0"/>
      <xs:element name="name" type="types:distinguishedNameExtType"/>
      <xs:element name="status" type="types:intervalStatusType" maxOccurs="99"/>
      <xs:element name="term" type="bn.types:termType" minOccurs="0"/>
      <xs:element name="previousStateOrTerritory" type="bn.types:previousStateTerritoryType" minOccurs="0"/>
      <xs:element name="jurisdiction" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="dateRegistered" type="xs:date" minOccurs="0"/>
      <xs:element name="dateRenewal" type="xs:date" minOccurs="0"/>
      <xs:element name="dateDeregistered" type="xs:date" minOccurs="0"/>
      <xs:element name="address" type="types:addressExtType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="emailAddress" type="types:emailType" minOccurs="0"/>
      <xs:element name="smsNumber" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="representative" type="bn.types:representativeType" minOccurs="0" maxOccurs="99"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bnLodgeFormType">
    <xs:annotation>
      <xs:documentation>Form BZ1 definition</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="types:distinguishedNameType"/>
      <xs:element name="term" type="bn.types:termType"/>
      <xs:element name="dateRenewal" type="xs:date" minOccurs="0"/>
      <xs:element name="address" type="types:addressLodgeType" minOccurs="0" maxOccurs="9"/>
      <xs:element name="emailAddress" type="types:emailType" nillable="true"/>
      <xs:element name="smsNumber" type="xs:string" nillable="true" minOccurs="0"/>
      <xs:element name="businessEntity" type="bn.types:businessEntityLodgeType" minOccurs="0"/>
      <xs:element name="transferKey" type="xs:string" minOccurs="0"/>
      <xs:element name="contact" type="bn.types:contactPersonType" minOccurs="0"/>
      <xs:element name="signatory" type="types:signatoryType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bnRenewalFormType">
    <xs:annotation>
      <xs:documentation>BN Renewal Form definition</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="accountNumber" type="types:accountNumberType">
          <xs:annotation>
            <xs:documentation>Account number of the Business name</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ABN" type="types:abnType"/>
        <xs:element name="transactionId" type="types:transactionIdType">
          <xs:annotation>
            <xs:documentation>TransactionId for the BN renewal request.
											 NOT IN CURRENT IMPLEMENTATION.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="businessName" type="types:distinguishedNameType">
        <xs:annotation>
          <xs:documentation>Business name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="term" type="bn.types:termType">
        <xs:annotation>
          <xs:documentation>The duration of the next registration period for the nominated business name. It can be one or three years.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="paymentType" type="types:paymentMethodType">
        <xs:annotation>
          <xs:documentation>Specify the payment option. Currently this is restricted to invoicing (INV) to the BN account only.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="emailAddress" type="types:emailType" nillable="true">
        <xs:annotation>
          <xs:documentation>Contact email address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="signatory" type="types:signatoryType">
        <xs:annotation>
          <xs:documentation>~ExternalMandatory~</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bnUpdateFormType">
    <xs:annotation>
      <xs:documentation>Form BZ4 definition</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="number" type="bn.types:bnIdentifier"/>
      <xs:element name="name" type="types:distinguishedNameType"/>
      <xs:element name="term" type="bn.types:termType" minOccurs="0"/>
      <xs:element name="dateRegistered" type="xs:date" minOccurs="0"/>
      <xs:element name="dateRenewal" type="xs:date" minOccurs="0"/>
      <xs:element name="address" type="types:addressType" minOccurs="0" maxOccurs="9"/>
      <xs:element name="emailAddress" type="types:emailType" nillable="true" minOccurs="0"/>
      <xs:element name="smsNumber" nillable="true" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="representative" type="bn.types:representativeType" minOccurs="0" maxOccurs="99"/>
      <xs:element name="businessEntity" type="bn.types:businessEntityType" minOccurs="0"/>
      <xs:element name="signatory" type="types:signatoryType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bnCancelFormType">
    <xs:annotation>
      <xs:documentation>Form BZ5 definition
BZ5A Owner cancellation
BZ5C Staff cancellation
BZ5D ABR cancellation</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="businessName" type="bn.types:bnIdentifier"/>
      <xs:element name="transfer" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Transfers every business name identified by abn</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="signatory" type="types:signatoryType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~ExternalMandatory~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fileNotes" type="types:fileNotesType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Internal~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="returnExtract" type="types:trueType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Internal~</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="bnLinkFormType">
    <xs:sequence>
      <xs:choice>
        <xs:element name="abn" type="types:abnType">
          <xs:annotation>
            <xs:documentation>abn of business name owner</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="nniNumber" type="types:nniNumberType"/>
      </xs:choice>
      <xs:element name="agent" type="types:agentType"/>
      <xs:element name="action">
        <xs:annotation>
          <xs:documentation>ADD or CEASE</xs:documentation>
          <xs:documentation>true  organisation is currently registered</xs:documentation>
          <xs:documentation>false  organisation is not currently registered</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="5"/>
            <xs:enumeration value="ADD"/>
            <xs:enumeration value="CEASE"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="signatory" type="types:signatoryType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~ExternalMandatory~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fileNotes" type="types:fileNotesType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Internal~</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="individualType">
    <xs:sequence>
      <xs:element name="name" type="types:personNameType"/>
      <xs:element name="previousName" type="types:personNameType" minOccurs="0"/>
      <xs:element name="birthDetails" type="types:birthDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Private~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="address" type="types:addressExtType" minOccurs="0"/>
      <xs:element name="emailAddress" type="types:emailType" nillable="true" minOccurs="0"/>
      <xs:element name="changeOfIndividual" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Change Of Details only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="startDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="bnStatusCodeType">
    <xs:annotation>
      <xs:documentation>APPL Reconsideration
DRGD  Deregistered
EXPD Expiring
MCON Ministerial Consent
PEND Awaiting Payment
REGD Registered
RJCT Rejected
SNSD Referred
SOFF Strike Off</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="4"/>
      <xs:enumeration value="APPL"/>
      <xs:enumeration value="EXPD"/>
      <xs:enumeration value="DRGD"/>
      <xs:enumeration value="MCON"/>
      <xs:enumeration value="PEND"/>
      <xs:enumeration value="REGD"/>
      <xs:enumeration value="RJCT"/>
      <xs:enumeration value="SNSD"/>
      <xs:enumeration value="SOFF"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="bnApplicationStatusCodeType">
    <xs:annotation>
      <xs:documentation>APPL Reconsideration
MCON Ministerial Consent
PEND Awaiting Payment
SNSD Referredf</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="4"/>
      <xs:enumeration value="APPL"/>
      <xs:enumeration value="MCON"/>
      <xs:enumeration value="PEND"/>
      <xs:enumeration value="SNSD"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="bnSearchStatusCodeType">
    <xs:annotation>
      <xs:documentation>DRGD Deregistered
PEND Pending Payment
REGD Registered</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="4"/>
      <xs:enumeration value="DRGD"/>
      <xs:enumeration value="PEND"/>
      <xs:enumeration value="REGD"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="businessEntityType">
    <xs:sequence>
      <xs:element name="abrEntity" type="abn.types:abrEntityType" minOccurs="0"/>
      <xs:element name="abnExemption" type="types:trueType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Xmas Island and Cocos Islands residents</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice minOccurs="0">
        <xs:element name="individual" type="bn.types:individualType">
          <xs:annotation>
            <xs:documentation>Sole Trader
Individual</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="organisation" type="bn.types:organisationType">
          <xs:annotation>
            <xs:documentation>Company
Partnership
UnincorporateStructure</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="ownerType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>IND - Individual/Sole trader
IB - Incorporated Body
PTSH - Partnership
USTR - Unincoporated Structure
JV - Joint Venture</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
            <xs:enumeration value="IND"/>
            <xs:enumeration value="IB"/>
            <xs:enumeration value="PTSH"/>
            <xs:enumeration value="USTR"/>
            <xs:enumeration value="JV"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="associate" type="bn.types:associateType" minOccurs="0" maxOccurs="500">
        <xs:annotation>
          <xs:documentation>Partners for Partnerships
Reps for Trusts etc
Ventures for Joint Venturers</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dateReview" type="xs:date" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="associateType">
    <xs:sequence>
      <xs:element name="abrEntity" type="abn.types:abrEntityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Joint Ventures only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="individual" type="bn.types:individualType">
          <xs:annotation>
            <xs:documentation>Sole Trader
Individual</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="organisation" type="bn.types:organisationType">
          <xs:annotation>
            <xs:documentation>Company
Partnership
UnincorporateStructure</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="startDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>~Outbound~</xs:documentation>
        </xs:annotation>
      </xs:element>
