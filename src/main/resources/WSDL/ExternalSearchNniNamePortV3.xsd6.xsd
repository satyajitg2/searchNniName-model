<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="uri:business.document.header.types.asic.gov.au" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:fss.types="uri:fss.types.asic.gov.au" xmlns:types="uri:types.asic.gov.au" xmlns:business.document.header.types="uri:business.document.header.types.asic.gov.au" xmlns:xmime="http://www.w3.org/2005/05/xmlmime" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="uri:types.asic.gov.au" schemaLocation="ExternalSearchNniNamePortV3.xsd3.xsd"/>
  <xs:import namespace="uri:fss.types.asic.gov.au" schemaLocation="ExternalSearchNniNamePortV3.xsd9.xsd"/>
  <xs:import namespace="http://www.w3.org/2005/05/xmlmime" schemaLocation="ExternalSearchNniNamePortV3.xsd10.xsd"/>
  <xs:complexType name="businessDocumentRequestHeaderType">
    <xs:sequence>
      <xs:element name="messageType">
        <xs:simpleType>
          <xs:restriction base="xs:string">
